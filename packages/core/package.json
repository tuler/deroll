{
    "name": "@deroll/core",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "license": "MIT",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "run-s codegen compile",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "codegen": "ts-node schema.ts",
        "compile": "tsup",
        "dev": "tsup --watch",
        "prepack": "run-s build"
    },
    "dependencies": {
        "viem": "^2.21.34"
    },
    "devDependencies": {
        "@deroll/tsconfig": "workspace:*",
        "@types/node": "^22.7.9",
        "npm-run-all": "^4",
        "openapi-typescript": "^7.4.4",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3",
        "vitest": "^2.1.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
