{
    "name": "@deroll/core",
    "version": "0.2.0",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "license": "MIT",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "run-s codegen compile",
