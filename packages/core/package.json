    },
    "devDependencies": {
        "@deroll/tsconfig": "workspace:*",
        "@types/node": "^20.14.11",
        "eslint": "^8.57.0",
        "eslint-config-deroll": "workspace:*",
        "npm-run-all": "^4",
        "openapi-typescript": "^7.1.0",
        "ts-node": "^10.9.2",
        "tsup": "^8.1.2",
        "typescript": "^5.5.3",
        "vitest": "^2.0.3"
    },
    "publishConfig": {
        "access": "public"
    }
